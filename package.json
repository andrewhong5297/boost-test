{
  "name": "boost-test",
  "version": "1.0.0",
  "description": "A simple TypeScript project",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --sourcemap",
    "start": "node dist/index.js",
    "dev": "tsup src/index.ts --sourcemap --watch --onSuccess \"node --enable-source-maps dist/index.js\"",
    "watch": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "typescript",
    "node"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@boostxyz/sdk": "7.5.0",
    "@types/node": "22.5.4",
    "prettier": "^3.2.5",
    "tsup": "8.2.4",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.14.0",
    "@wagmi/core": "^2.17.3",
    "viem": "^2.31.4"
  }
}